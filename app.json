{
  "expo": {
    "name": "Romza Great Places",
    "description": "Romza app for cataloging great places all over the world.",
    "slug": "romza-great-places",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/places.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.romzalabs.romza-great-places",
      "supportsTablet": true
    },
    "android": {
      "package": "com.romzalabs.romza-great-places",
      "adaptiveIcon": {
        "foregroundImage":  "./assets/places-adaptive.png",
        "backgroundColor": "#171717"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "googleMaps": {
        "apiKey": "REDACTED"
      }
    }
  }
}
